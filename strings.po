msgctxt "STRINGS.CODEX.TIMEOFFAPPROVED.BODY.EMAILHEADER"
msgid "<smallcaps>To: <b>Dr. Ross</b><size=12><alpha=#AA> <dross@gravitas.nova></size></color>\nFrom: <b>Admin</b><size=12><alpha=#AA> <admin@gravitas.nova></color></size></smallcaps>\n------------------\n"
msgstr "<smallcaps>ถึง: <b>ดร.รอส</b> <alpha=#AA><size=12><dross@gravitas.nova></size></color>\nจาก: <b>แอดมิน</b> <alpha=#AA><size=12><admin@gravitas.nova></size></color></smallcaps>\n------------------\n"

